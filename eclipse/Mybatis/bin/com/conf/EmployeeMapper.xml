<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 		PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis01.dao.EmployeeMapper">
	<select id="getEmpById" resultType="com.mybatis01.bean.Employee">
	    select id,last_name lastName,email,gender from tbl_employee where id = #{id}	    
	</select>
	
	<select id="getAllById" resultType="com.mybatis01.bean.Employee">
	    select * from tbl_employee 	    
	</select>
		
	<insert id="insertEmpById" resultType="com.mybatis01.bean.Employee">
	    insert into tbl_employee (last_name lastName,email,gender) value ("mmb3","111@123.com",0 )
	</insert>
</mapper> 
    
 